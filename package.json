{
  "devDependencies": {
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "postcss": "^8.4.45",
    "postcss-cli": "^11.0.0",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "openai": "^4.67.3",
    "together-ai": "^0.6.0-alpha.4"
  },
  "scripts": {
    "start": "npm run build-css & npm run dev",
    "build-css": "tailwindcss build -i ./web/input.css -o ./web/output.css",
    "watch-css": "tailwindcss build -i ./web/input.css -o ./web/output.css --watch",
    "dev": "npx tailwindcss -i ./web/input.css -o ./web/output.css --watch"
  },
  "type": "module"
}
